{
  "name": "@devantage/n8n-nodes-word-docx-metadata-extractor",
  "version": "1.0.0",
  "description": "n8n community nodes for Word DOCX metadata extraction",
  "contributors": [
    "Guilherme Nogueira <guilherme.nogueira@devantage.com.br>"
  ],
  "homepage": "https://github.com/devantage/n8n-nodes-word-docx-metadata-extractor",
  "keywords": [
    "n8n-community-node-package",
    "msword",
    "word",
    "docx",
    "metadata"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/WordDocxMetadataExtractor.node.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy:assets": "copyfiles icons/icon.svg dist/",
    "build:compile": "tsc",
    "build": "npm run clean && npm run build:compile && npm run copy:assets",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "unzipper": "^0.12.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/unzipper": "^0.10.11",
    "@types/xml-js": "^0.9.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
